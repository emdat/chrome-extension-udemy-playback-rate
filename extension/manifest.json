{
	"name": "Udemy Video Playback Speed",
	"version": "1.0.0",
	"description": "Adds more playback speed options for videos on Udemy",
	"manifest_version": 2,
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon128.png"
	},
	"permissions": ["storage"],
	"content_scripts": [
		{
			"matches": ["https://www.udemy.com/*"],
			"exclude_matches": [
				"https://www.udemy.com/",
				"https://www.udemy.com/user/*",
				"https://www.udemy.com/dasboard/*",
				"https://www.udemy.com/cart/*",
				"https://www.udemy.com/courses/*",
				"https://www.udemy.com/home/*",
				"https://www.udemy.com/message/*"
			],
			"js": ["playbackRate.js"]
		}
	]
}
